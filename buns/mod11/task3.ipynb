   "source": [
    "import numpy as np\n",
    "\n",
    "a = np.random.randint(1, 10, size=(5, 5))\n",
    "\n",
    "def np_diag_2k(a):\n",
    "    res = sum([m for m in np.diagonal(a) if m % 2 == 0])\n",
    "    return res\n",
    "\n",
    "np_diag_2k(a)"
   ]
  }
